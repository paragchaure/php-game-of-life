{
  "name": "paragchaure/php-game-of-life",
  "require": {
    "ext-simplexml": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "php": ">=8.1",
    "symfony/console": "^6"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Life\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Life\\": "tests/"
    }
  },
  "scripts": {
    "test": "php vendor/bin/phpunit tests"
  }
}
